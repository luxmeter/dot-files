{
  "python.sortImports.path": "isort",
  "python.venvPath": "~/.virtualenvs",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 0,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "terminal.integrated.rendererType": "dom",
  "explorer.confirmDelete": false,
  "editor.detectIndentation": true,
  "editor.tabSize": 2,
  "editor.fontFamily": "'Hack', 'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "after": ["\"", "_", "d", "P"]
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"]
    },
    {
      "before": [">"],
      "after": [">", "g", "v"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["[", "<space>"],
      "after": ["O", "<c-[>", "j"]
    },
    {
      "before": ["]", "<space>"],
      "after": ["o", "<c-[>", "k"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["[", "e"],
      "after": ["d", "d", "k", "P"]
    },
    {
      "before": ["]", "e"],
      "after": ["d", "d", "p"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "breadcrumbs.enabled": true,
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "workbench.sideBar.location": "left",
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": false,
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "default",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.fontSize": 14,
  "files.exclude": {
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true
  },
"python.jediEnabled": false,
  "python.pythonPath": "/Users/caylak/.virtualenvs/nvimpy3/bin/python",
  "workbench.colorTheme": "Visual Studio Dark",
  "workbench.iconTheme": "Monokai Pro Icons",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "python.formatting.provider": "black",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "editor.multiCursorModifier": "ctrlCmd",
"security.workspace.trust.untrustedFiles": "open",
"jupyter.askForKernelRestart": false,
"terminal.external.osxExec": "Alacritty.app",
"terminal.integrated.fontFamily": "Monoid Nerd Font Mono",
"terminal.integrated.fontSize": 13,
"editor.accessibilitySupport": "off",
"explorer.confirmDragAndDrop": false
}
